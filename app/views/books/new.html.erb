<h1>Books</h1>
<table>
  <thead>
  <tr>
  <th>Title</th>
  <th>Body</th>
  <th colspan="3"></th>
  </tr>
  </thead>
  </table>

 


<h2>New Book</h2>
<!-- form_with部分 -->
<%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Book' %>
<% end %>